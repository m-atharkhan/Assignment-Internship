{
    "version": 2,
    "builds": [
      { 
        "src": "./api/index.js", 
        "use": "@vercel/node" ,
        "config":{
          "maxDuration": 60,
          "memory": 1024
        }
      }
    ],
    "routes": [
      { 
        "src": "/(.*)", 
        "dest": "/api/index.js"
     }
    ]
  }